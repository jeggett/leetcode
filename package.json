{
  "name": "leetcode",
  "version": "1.0.0",
  "description": "Repo created to track progress on leetcode",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jeggett/leetcode.git"
  },
  "keywords": [
    "leetcode"
  ],
  "author": "Alexander Zaytsev",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/jeggett/leetcode/issues"
  },
  "homepage": "https://github.com/jeggett/leetcode#readme",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
