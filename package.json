{
  "name": "leetcode",
  "version": "1.0.0",
  "description": "Repo created to track progress on leetcode",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jeggett/leetcode.git"
  },
  "keywords": [
    "leetcode"
  ],
