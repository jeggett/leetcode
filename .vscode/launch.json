{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: [1](https://code.visualstudio.com/docs/typescript/typescript-debugging)
  "version": "0.2.0",
  "configurations": [
    {
      "name": "TypeScript",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/build/**/*.js"]
    },
    {
      // This configuration will run the current Python file with Python
      "name": "Python",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
